{
	"manifest_version": 2,

	"name": "FIFO",
	"description": "First bookmark in, first bookmark out",
	"developer": {
		"name": "Joethy",
		"url": "https://github.com/Joethy"
	},
	"icons": {
		"48": "icon48.png",
		"128": "icon128.png"
	},
	"version": "1.0",
	"background": {
		"scripts": ["background.js"]
	},
	"permissions": ["bookmarks","tabs"],
	"browser_action": {
		"default_icon": "icon48.png",
		"default_title": "FIFO",
		"default_popup":"popup.html"
	}
}